{
  "name": "amane_moderation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "up": "npm-run-all build start",
    "build": "tsc -p . && tsc-alias",
    "start": "node --trace-warnings dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./src/* --ignore-path .prettierignore --max-warnings 0",
    "lint:typecheck": "tsc --pretty --noEmit",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "glob": "7.2.3"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "tsc-alias": "^1.8.8",
    "tsc-paths-resolver": "^0.1.3",
    "typescript": "^5.2.2"
  }
}
